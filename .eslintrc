{
  "extends": "eslint-config-rackt",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2,
    "jsx-quotes": [2, "prefer-single"],
    "comma-dangle": [2, "always-multiline"],
    "space-before-function-paren": [1, "never"]
  },
  "plugins": [
    "babel",
    "react"
  ]
}
